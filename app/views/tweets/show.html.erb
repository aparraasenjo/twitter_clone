<p id="notice"><%= notice %></p>

<p>
  <strong>Content:</strong>
  <%= @tweet.content %>
</p>

<p>
  <strong>Likes:</strong>
  <ul>
    <% @tweet.likes.each do |like| %>
      <li><img src="<%= like.user.avatar %>" alt="user avatar"></li>
    <% end %>
  </ul>
</p>



<p>
  <strong>User:</strong>
  <%= @tweet.user %>
</p>

<%= link_to 'Edit', edit_tweet_path(@tweet) %> |
<%= link_to 'Back', root_path %>
